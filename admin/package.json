  "private": false,
  "scripts": {
    "postinstall": "./node_modules/bower/bin/bower install && ./node_modules/requirejs/bin/r.js -o public/app.build.js",
    "start": "node server.js"
  },
  "dependencies": {
    "bower": "*",
    "express": "3.x",
    "gridfs-stream": "*",
    "moment": "*",
    "mongoose": "*",
    "mongoose-when": "*",
    "uinexpress": "*",
    "underscore": "*",
    "knox": "*",
    "sha1": "^1.1.0",
    "mongodb": "^1.4.0",
